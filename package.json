{
  "name": "mrt-service-user",
  "version": "1.0.0",
  "description": "User service for MRT simulator",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f config/.env.development ts-node-dev --respawn -r tsconfig-paths/register src/index.ts",
    "start-tcp": "env-cmd -f config/.env.development ts-node-dev --respawn -r tsconfig-paths/register src/tcp.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "prod": "node ./prod/build-addon",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "resolution": "tsc --traceResolution"
  },
  "author": "dwiargodirgantoro@gmail.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.6",
    "body-parser": "^1.20.2",
    "composable-middleware": "^0.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "jwt-decode": "^3.1.2",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.0",
    "objection": "3.0.1",
    "objection-find": "3.0.1",
    "objection-visibility": "1.2.0",
    "pg": "^8.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.1",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.39.0",
    "fs-extra": "^11.1.1",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.0.4"
  }
}
